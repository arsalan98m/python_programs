    "phone = input(\"Phone: \")\n",
    "\n",
    "dic = {\n",
    "    \"1\": \"One\",\n",
    "    \"2\": \"Two\",\n",
    "    \"3\": \"Three\",\n",
    "    \"4\": \"Four\",\n",
    "    \"5\": \"Five\",\n",
    "}\n",
    "output = \"\"\n",
    "for char in phone:\n",
    "    if char in dic.keys():\n",
    "        output += dic[char] + \" \"\n",
    "    else:\n",
    "        output+= \"!\"\n",
    "print(output)\n"
